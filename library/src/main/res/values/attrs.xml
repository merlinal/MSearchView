<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--自定义搜索-->
    <declare-styleable name="MSearchView">
        <attr name="fromColor" format="color" />
        <attr name="toColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="hintColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="searchShrinkIcon" format="reference" />
        <attr name="searchExpandIcon" format="reference" />
        <attr name="clearIcon" format="reference" />
        <attr name="expanded" format="boolean" />
        <attr name="hint" format="string"/>
    </declare-styleable>

</resources>